{
  "name": "api-rest",
  "displayName": "REST API",
  "description": "RESTful API with Node.js, Express, TypeScript, and best practices",
  "version": "1.0.0",
  "category": "backend",
  "languages": ["TypeScript", "JavaScript"],
  "frameworks": ["Express.js", "Prisma", "JWT"],
  "author": "Speccy-Kit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/username/{{projectName}}"
  },
  "keywords": ["api", "rest", "nodejs", "express", "typescript", "prisma"],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "features": [
    "Express.js with TypeScript",
    "Prisma ORM for database management",
    "JWT authentication and authorization",
    "Comprehensive API documentation with Swagger/OpenAPI",
    "Input validation with Joi or Zod",
    "Error handling and logging",
    "Rate limiting and security middleware",
    "Unit and integration testing with Jest",
    "Docker containerization",
    "Environment-based configuration"
  ],
  "structure": {
    "src": {
      "controllers": "Route controllers and request handlers",
      "routes": "API route definitions",
      "middleware": "Custom middleware functions",
      "services": "Business logic and data processing",
      "models": "Data models and schemas",
      "utils": "Utility functions and helpers",
      "types": "TypeScript type definitions",
      "config": "Configuration management",
      "tests": "Test files and fixtures",
      "docs": "API documentation"
    },
    "prisma": "Database schema and migrations",
    "docker": "Docker configuration files",
    "scripts": "Build and deployment scripts"
  },
  "dependencies": {
    "express": "^4.18.0",
    "typescript": "^5.0.0",
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "cors": "^2.8.5",
    "@types/cors": "^2.8.0",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "@types/compression": "^1.7.0",
    "morgan": "^1.10.0",
    "@types/morgan": "^1.9.0",
    "dotenv": "^16.0.0",
    "bcryptjs": "^2.4.3",
    "@types/bcryptjs": "^2.4.0",
    "jsonwebtoken": "^9.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "express-rate-limit": "^7.0.0",
    "joi": "^17.9.0",
    "prisma": "^5.0.0",
    "@prisma/client": "^5.0.0",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-express": "^5.0.0",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/supertest": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.0"
  },
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node src/scripts/seed.ts",
    "docker:build": "docker build -t {{projectName}} .",
    "docker:run": "docker run -p 3000:3000 {{projectName}}"
  },
  "configurations": {
    "express": {
      "port": 3000,
      "cors": {
        "origin": ["http://localhost:3000", "http://localhost:3001"],
        "credentials": true
      },
      "rateLimit": {
        "windowMs": 900000,
        "max": 100
      }
    },
    "prisma": {
      "schema": "prisma/schema.prisma",
      "database": "postgresql://user:password@localhost:5432/mydb"
    },
    "jest": {
      "preset": "ts-jest",
      "testEnvironment": "node",
      "roots": ["<rootDir>/src"],
      "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
      "collectCoverageFrom": [
        "src/**/*.ts",
        "!src/**/*.d.ts",
        "!src/tests/**",
        "!src/scripts/**"
      ]
    },
    "eslint": {
      "extends": [
        "@typescript-eslint/recommended",
        "prettier"
      ],
      "rules": {
        "@typescript-eslint/no-unused-vars": "error",
        "@typescript-eslint/explicit-function-return-type": "warn"
      }
    }
  },
  "apiStructure": {
    "version": "v1",
    "basePath": "/api/v1",
    "endpoints": [
      {
        "path": "/auth",
        "methods": ["POST"],
        "description": "Authentication endpoints (login, register, refresh)"
      },
      {
        "path": "/users",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "description": "User management endpoints"
      },
      {
        "path": "/health",
        "methods": ["GET"],
        "description": "Health check endpoint"
      }
    ]
  },
  "middleware": [
    "helmet",
    "cors",
    "compression",
    "morgan",
    "express.json()",
    "express.urlencoded({ extended: true })",
    "rateLimit",
    "errorHandler"
  ],
  "authentication": {
    "strategy": "JWT",
    "accessTokenExpiry": "15m",
    "refreshTokenExpiry": "7d",
    "algorithm": "HS256"
  },
  "validation": {
    "library": "joi",
    "schemas": ["auth", "users", "common"]
  },
  "database": {
    "type": "postgresql",
    "orm": "prisma",
    "migrations": true,
    "seeding": true
  },
  "testing": {
    "unit": true,
    "integration": true,
    "e2e": false,
    "coverage": {
      "threshold": {
        "global": {
          "branches": 80,
          "functions": 80,
          "lines": 80,
          "statements": 80
        }
      }
    }
  },
  "docker": {
    "baseImage": "node:18-alpine",
    "workingDir": "/app",
    "exposePort": 3000,
    "healthCheck": {
      "endpoint": "/health",
      "interval": "30s",
      "timeout": "3s"
    }
  },
  "environment": {
    "development": {
      "NODE_ENV": "development",
      "PORT": "3000",
      "DATABASE_URL": "postgresql://user:password@localhost:5432/devdb",
      "JWT_SECRET": "dev-secret-key",
      "JWT_EXPIRES_IN": "7d",
      "LOG_LEVEL": "debug"
    },
    "production": {
      "NODE_ENV": "production",
      "PORT": "3000",
      "DATABASE_URL": "{{DATABASE_URL}}",
      "JWT_SECRET": "{{JWT_SECRET}}",
      "JWT_EXPIRES_IN": "15m",
      "LOG_LEVEL": "info"
    }
  },
